<?xml version="1.0" encoding="utf-8"?>
<resources>

  <style name="FullScreenDialog" parent="Theme.AppCompat.Dialog">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowFullscreen">false</item>
    <item name="android:windowIsFloating">false</item>
    <item name="android:windowBackground">@color/dark_transparent_10</item>
    <item name="android:statusBarColor">@color/transparent</item>

  </style>

  <style name="LowerCaseMenuTextAppearance" parent="android:TextAppearance.Holo.Widget.ActionBar.Menu">
    <item name="android:textAllCaps">false</item>
    <item name="android:textSize">17sp</item>
    <item name="android:fontFamily">@font/regular</item>
    <item name="android:lineSpacingExtra">7sp</item>
  </style>

  <style name="Title20Sb24L">
    <item name="android:fontFamily">@font/medium</item>
    <item name="android:textSize">20sp</item>
    <item name="android:lineSpacingExtra">0sp</item>
    <item name="android:textColor">@color/black</item>
  </style>

</resources>
